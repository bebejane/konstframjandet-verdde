{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "module": "CommonJS",
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "/components"
      ],
      "graphql/*": [
        "/graphql/"
      ],
      "lib/*": [
        "/lib"
      ],
      "/*": [
        "*"
      ]
    }
  },
  "typeRoots": [
    "./@types"
  ],
  "include": [
    "next-env.d.ts",
    "global.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "ts-node": {
    "files": true
  }
}